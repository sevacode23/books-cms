  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:cov": "cross-env NODE_ENV=test vitest run --coverage",
    "test:debug": "cross-env NODE_ENV=test vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "cross-env NODE_ENV=test vitest run --config ./vitest.config.e2e.ts",
    "migration:generate": "cross-env NODE_ENV=development typeorm-ts-node-commonjs -d src/database/typeorm-cli.config.ts migration:generate",
    "migration:create": "cross-env NODE_ENV=development typeorm migration:create",
    "migration:run": "cross-env NODE_ENV=development typeorm-ts-node-commonjs -d src/core/database/typeorm-cli.config.ts migration:run",
    "migration:revert": "cross-env NODE_ENV=development typeorm-ts-node-commonjs -d src/core/database/typeorm-cli.config.ts migration:revert"
  },
